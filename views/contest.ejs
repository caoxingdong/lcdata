<%- include("partials/header") %>

<div class="container">
	<h1>contest page</h1>
	<table class="table">
  <thead>
    <tr>
      <th scope="col">UserName</th>
      <th scope="col">Rank</th>
      <th scope="col">Country</th>
    </tr>
  </thead>
  <tbody>
	  <% data.forEach(function(user){ %>
    <tr>
      <td><%= user.username%></td>
      <td><%= user.rank%></td>
      <td><%= user.country_name%></td>
    </tr>
	  <% }); %>
	    </tbody>
</table>
</div>


<%- include("partials/footer") %>
